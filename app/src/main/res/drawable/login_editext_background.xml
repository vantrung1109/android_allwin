<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="#E9EFF6"
                android:centerColor="#E9EFF6" android:endColor="#E9EFF6"
                android:angle="270" android:centerX="0.5" android:centerY="0.5" />
            <stroke android:width="1dp" android:color="#277CFB" />
            <corners android:radius="@dimen/_10sdp" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#E9EFF6"
                android:centerColor="#E9EFF6" android:endColor="#E9EFF6"
                android:angle="270" android:centerX="0.5" android:centerY="0.5" />
            <stroke android:width="1dp" android:color="#277CFB" />
            <corners android:radius="@dimen/_10sdp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#E9EFF6"
                android:centerColor="#E9EFF6" android:endColor="#E9EFF6"
                android:angle="270" android:centerX="0.5" android:centerY="0.5" />
            <stroke android:width="1dp" android:color="#E9EFF6" />
            <corners android:radius="@dimen/_10sdp" />
        </shape>
    </item>

</selector>